				},
				{
					"command": "godotTools.scenePreview.copyNodePath",
					"when": "false"
				},
				{
					"command": "godotTools.scenePreview.copyResourcePath",
					"when": "false"
				},
				{
					"command": "godotTools.scenePreview.openScript",
					"when": "false"
				},
